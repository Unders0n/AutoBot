    </handlers></system.webServer>
  <nlog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target name="logfile" xsi:type="File" layout="${longdate} ${message} ${exception:format=toString,Data:maxInnerExceptionLevel=10}" fileName="${basedir}/logs/${date:format=yyyy-MM-dd}-webapi.log" />
      <target name="eventlog" xsi:type="EventLog" layout="${longdate} ${message} ${exception:format=tostring}" log="Application" source="Flow" />
    </targets>
    <rules>
      <logger name="*" minlevel="Trace" writeTo="logfile" />
      <logger name="*" minlevel="Trace" writeTo="eventlog" />
    </rules>
  </nlog>
